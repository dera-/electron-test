{
  "name": "electron-quick-start",
  "version": "1.0.8",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64 --publish never",
    "publish": "electron-builder --win --x64 --publish always"
  },
  "repository": "https://github.com/dera-/electron-test",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "electron-log": "^5.0.0",
    "electron-updater": "^6.1.4",
    "update-electron-app": "^3.0.0"
  },
  "build": {
    "appId": "electron-quick-start",
    "win": {
      "icon": "build/style/img/icon.png",
      "publish": {
        "provider": "github",
        "owner": "dera-"
      }
    }
  }
}
